  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrent --kill-others \"npm run wpk\" \"npm run start-dev\"",
    "wpk-dev": "better-npm-run wpk-dev",
    "wpk-prod": "better-npm-run wpk-prod",
    "start-dev": "better-npm-run start-dev",
    "lint": "eslint -c .eslintrc src"
  },
  "betterScripts": {
    "start-dev": {
      "command": "node ./bin/www.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8000,
        "APIPORT": 8030
      }
    },
    "wpk-dev": {
      "command": "webpack --config webpack.config.dev.js",
      "env": {
        "NODE_ENV": "development",
        "PORT": 8000
      }
    },
    "wpk-prod": {
      "command": "webpack -p --config webpack.config.prod.js",
      "env": {
        "NODE_ENV": "production",
        "PORT": 8000
      }
    }
  },
